{
  "build": {
    "content": [
      {
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "_themes/**",
          "_themes.pdf/**",
          "**/docfx.json",
          "_repo.en-us/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices.md"
        ],
        "files": [
          "**/*.md",
          "**/*.yml"
        ]
      }
    ],
    "resource": [
      {
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "_themes/**",
          "_themes.pdf/**",
          "**/docfx.json",
          "_repo.en-us/**"
        ],
        "files": [
          "**/*.png",
          "**/*.jpg"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "feedback_system": "None",
      "uhfHeaderId": "MSDocsHeader-Lifecycle"
    },
    "fileMetadata": {
      "author": {
        "**/*.md": "github-name",
        "**/*.yml": "github-name"
      },
      "hide_bc": {
        "**/*.md": "true",
        "**/*.yml": "true"
      },
      "ms.author": {
        "**/*.md": "mslpol",
        "**/*.yml": "mslpol"
      },
      "ms.prod": {
        "**/*.md": "lifecycle",
        "**/*.yml": "lifecycle"
      },
      "ms.topic": {
        "**/*.md": "lifecycle",
        "**/*.yml": "lifecycle"
      },
      "titleSuffix": {
        "**/*.md": "Microsoft Lifecycle",
        "**/*.yml": "Microsoft Lifecycle"
      },
      "toc_rel": {
        "**/end-of-support/*.md": "/lifecycle/toc.json"
      }
    },
    "template": [],
    "dest": "lifecycle-data",
    "markdownEngineName": "markdig"
  }
}